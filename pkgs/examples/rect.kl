
pub class Rect {
    var x int32
    pub var y int32

    func init() {

    }

    pub func set_x(x int32) {
        self.x = x
    }

    pub func get_x() int32 {
        self.x
    }

    pub func area() int32 {
        x * y
    }

    pub func to_string() string {
        "area($xx$y)=${area()}"
    }
}
