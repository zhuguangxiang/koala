func show_valist(args ...) {
	len := args.len()
	for i in 0 ..< len {
		fmt.println(args[i])
	}
}
show_valist(1,2)

func vavalist(args ...) {
		show_valist(args)
	  }
vavalist(1,2,3)

"{}\n{}".format(1,2)

import "sys"
func fprintf(w io.Writer, fmt string, args ...) {
  s := fmt.format(args)
  w.write_str(s)
  return
}
fprintf(sys.stdout, "hello\n")

w := sys.stdout
fprintf(w, "{}")
fprintf(w, "{}", "hello\n")
